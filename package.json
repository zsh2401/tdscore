{
  "name": "tdscore",
  "version": "0.1.27",
  "description": "Data Structure, algorithms, mathmatic codes and enhancement of ECMAScript written by TypeScript",
  "repository": "https://github.com/zsh2401/tdscore",
  "author": "Seymour Zhang<zsh2401@163.com>",
  "license": "MulanPSL2",
  "homepage": "http://ds.zsh2401.top",
  "main": "lib/index.js",
  "unpkg": "dist/tdscore.js",
  "typings": "lib/index.d.ts",
  "module": "es/index.js",
  "lint": "eslint src/*",
  "engines": {
    "node": ">= 6.4.0"
  },
  "scripts": {
    "build:dist": "cross-env TS_NODE_PROJECT=\"./tsconfig.webpack.json\" webpack --config ./webpack.config.ts",
    "build:qjs": "cross-env TS_NODE_PROJECT=\"./tsconfig.webpack.json\" webpack --config ./webpack.config.qjs.ts",
    "build:lib": "tsc",
    "build:es": "tsc --project tsconfig.es.json",
    "build": "npm run build:es && npm run build:dist && npm run build:lib && npm run build:qjs",
    "lint": "eslint src/",
    "release": "npm run build && npm publish",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "ci": "yarn install --frozen-lockfile",
    "prepare": "husky install"
  },
  "pre-commit": [
    "build",
    "test"
  ],
  "dependencies": {
    "bignumber.js": "^9.0.1",
    "sha256": "^0.2.0"
  },
  "devDependencies": {
    "@types/jest": "^26.0.22",
    "@types/sha256": "^0.2.0",
    "@types/terser-webpack-plugin": "^5.0.3",
    "@typescript-eslint/eslint-plugin": "^4.20.0",
    "@typescript-eslint/parser": "^4.20.0",
    "@webpack-cli/init": "^1.1.3",
    "codecov": "^3.8.1",
    "cross-env": "^7.0.3",
    "eslint": "^7.23.0",
    "husky": "^6.0.0",
    "jest": "^26.6.3",
    "pinst": "^2.1.6",
    "process": "^0.11.10",
    "terser-webpack-plugin": "^5.1.1",
    "threads-plugin": "^1.4.0",
    "ts-jest": "^26.5.4",
    "ts-loader": "^8.1.0",
    "ts-node": "^9.1.1",
    "typescript": "^4.2.4",
    "webpack": "^5.28.0",
    "webpack-bundle-analyzer": "^4.4.0",
    "webpack-cli": "^4.6.0"
  }
}
